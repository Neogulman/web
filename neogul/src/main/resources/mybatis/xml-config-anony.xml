<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xml-config-anony">
	<select id="view" resultType="com.joheul.data.dto.TB_Config_Anony">
		select 
			useYN
		 from tb_config_anony
	</select>
	
	<update id="update">
		update tb_config_anony set
			useYN=#{useYN}
	</update>
</mapper>