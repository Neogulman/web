<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xml-type-seq">
	

	<select id="list" resultType="com.joheul.data.dto.TB_Type_Seq">
		select 
			idx,
			type,
			seq,
			year
		 from tb_type_seq
		 where 1=1
		 <if test='idx != ""'>
		 		and idx =#{idx}
		 </if>
	</select>
	
	
	<update id="update">
		update tb_type_seq set
			type=#{type},
			seq=#{seq},
			year=#{year}
		where idx=#{idx}
	</update>

	
</mapper>