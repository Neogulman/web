<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xml-version-info">
	
	<select id="list" resultType="com.neogul.data.dto.TB_Version_Info">
		select
			type,
			version
		from 
			tb_version_info
		ORDER BY idx DESC
	</select>
	
</mapper>